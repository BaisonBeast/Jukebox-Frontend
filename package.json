{
  "name": "jukebox-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "network": "vite --mode network",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check --log-level warn \"src/**/*.{jsx,js,ts,tsx}\"",
    "format:fix": "prettier --write --log-level warn \"src/**/*.{jsx,js,ts,tsx}\"",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:preview": "npx vite preview --outDir test-html"
  },
  "dependencies": {
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-table": "^8.16.0",
    "ag-grid-react": "^32.1.0",
    "axios": "^1.6.2",
    "cross-env": "^7.0.3",
    "material-icons": "^1.13.12",
    "react": "^18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "recharts": "^2.10.2",
    "socket.io-client": "^4.7.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@development-suite/types": "^1.0.7",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "core-js": "^3.38.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "fake-indexeddb": "^6.0.0",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^2.0.5"
  }
}
